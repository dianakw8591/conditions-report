<% if flash[:notice] %>
    <%= flash[:notice]%><br>
<% end %>

<h1>Account Information</h1>
<div>Username: <%= @user.username %></div>
<div>Location: <%= @user.location.name %></div>

<%= button_to 'EDIT ACCOUNT', edit_user_path, method: :get%>
<%= button_to 'DELETE', '/users/delete', method: :delete%>
<%= button_to 'LOGOUT', '/logout', method: :delete%>
